<?xml version="1.0" encoding="utf-8"?>
<HelpXML>
   <Title><!--This is simply the filename-->cabc</Title>
   <Purpose><!--This is the second line of the .m file-->cabc closes all open figures except the one in foreground (the current)
</Purpose>
   <Description><!--Description section--></Description>
   <InpArgs><!--REQUIRED INPUT ARGUMENT SECTION--></InpArgs>
   <OptArgs><!--OPTIONAL (NAME/PAIRS) INPUT ARGUMENT SECTION--></OptArgs>
   <OutArgs><!--OUTPUT ARGUMENT SECTION--></OutArgs>
   <MoreAbout><!--MORE ABOUT SECTION--></MoreAbout>
   <Acknowledgements><!--ACKNOWLEDGEMENTS SECTION--></Acknowledgements>
   <References><!--REFERENCES SECTION-->
      <Item>Tufte E.R. (1983). The visual display of quantitative information. Graphics Press, Cheshire</Item>
   </References>
   <SeeAlso><!--SEE ALSO SECTION-->
      <Item>close</Item>
   </SeeAlso>
   <Ex><!--EXAMPLES SECTION-->
      <Item>
         <Title>Plot sin, cos and atan in the interval 0 2pi.</Title>
         <Desc> </Desc>
         <MATLABcode>
            <ItemCell>x = 0:pi/1000:2*pi;</ItemCell>
            <ItemCell>plot(x,sin(x));</ItemCell>
            <ItemCell>figure</ItemCell>
            <ItemCell>plot(x,cos(x));</ItemCell>
            <ItemCell>figure</ItemCell>
            <ItemCell>plot(x,atan(x))</ItemCell>
            <ItemCell>cascade</ItemCell>
            <ItemCell>% Now highlight a figure and then digit</ItemCell>
            <ItemCell>cabc</ItemCell>
            <ItemCell>% All the other figures but the one which has been selected will be</ItemCell>
            <ItemCell>% closed</ItemCell>
         </MATLABcode>
         <Exec>1</Exec>
      </Item>
   </Ex>
   <ExtraEx><!--EXTRA EXAMPLES SECTION-->
      <Item>
         <Title>Use of routine cabc inside GUI brushRES.</Title>
         <Desc> </Desc>
         <MATLABcode>
            <ItemCell>brushRES</ItemCell>
         </MATLABcode>
         <Exec>0</Exec>
      </Item>
   </ExtraEx>
</HelpXML>